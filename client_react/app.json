{
  "expo": {
    "name": "Bit URL",
    "slug": "BitURL",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/4br_96x.png",
    "short_name": "dibr.cc",
    "theme_color": "#2196f3",
    "background_color": "#2196f3",
    "display": "standalone",
    "Scope": "/home",
    "start_url": "/home",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2196f3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2196f3"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "homepage": "home"
    }
  }
}
